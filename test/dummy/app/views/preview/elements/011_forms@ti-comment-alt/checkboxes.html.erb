<div class="container-fluid">
  <div class="row">
    <div class="col-12">
      <div class="card">
        <div class="card-header">
          <h6>Basic Contextual Checkboxes</h6>
        </div>
        <div class="card-body">
          <fieldset>
            <div class="checkbox ">
              <input id="checkbox1" type="checkbox">
              <label for="checkbox1">
                Default
              </label>
            </div>
            <div class="checkbox  checkbox-primary">
              <input id="checkbox2" type="checkbox" checked="">
              <label for="checkbox2">
                Primary
              </label>
            </div>
            <div class="checkbox  checkbox-success">
              <input id="checkbox3" type="checkbox" checked>
              <label for="checkbox3">
                Success
              </label>
            </div>
            <div class="checkbox  checkbox-info">
              <input id="checkbox4" type="checkbox" checked>
              <label for="checkbox4">
                Info
              </label>
            </div>
            <div class="checkbox  checkbox-warning">
              <input id="checkbox5" type="checkbox" checked="">
              <label for="checkbox5">
                Warning
              </label>
            </div>
            <div class="checkbox  checkbox-danger">
              <input id="checkbox6" type="checkbox" checked="">
              <label for="checkbox6">
                Check me out
              </label>
            </div>
            <br>
            <p>Inline checkboxes</p>
            <div class="form-check form-check-inline checkbox">
              <input type="checkbox" id="inlineCheckbox1" value="option1">
              <label for="inlineCheckbox1"> Inline One </label>
            </div>
            <div class="form-check form-check-inline checkbox">
              <input type="checkbox" id="inlineCheckbox2" value="option1" checked="">
              <label for="inlineCheckbox2"> Inline Two </label>
            </div>
            <div class="form-check form-check-inline disabled checkbox">
              <input type="checkbox" id="inlineCheckbox3" value="option1">
              <label for="inlineCheckbox3"> Inline Three </label>
            </div>
          </fieldset>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-12">
      <div class="card">
        <div class="card-header">
          <h6>Circled Checkboxes</h6>
        </div>
        <div class="card-body">
          <fieldset>
            <div class="checkbox  checkbox-circle">
              <input id="checkbox7" type="checkbox">
              <label for="checkbox7">
                Simply Rounded
              </label>
            </div>
            <div class="checkbox  checkbox-info checkbox-circle">
              <input id="checkbox8" type="checkbox" checked="">
              <label for="checkbox8">
                Me too
              </label>
            </div>
          </fieldset>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-12">
      <div class="card">
        <div class="card-header">
          <h6>Disabled Checkboxes</h6>
        </div>
        <div class="card-body">
          <fieldset>
            <div class="checkbox  ">
              <input id="checkbox9" type="checkbox" disabled="">
              <label for="checkbox9">
                Can't check this
              </label>
            </div>
            <div class="checkbox  checkbox-success">
              <input id="checkbox10" type="checkbox" disabled="" checked="">
              <label for="checkbox10">
                This too
              </label>
            </div>
            <div class="checkbox  checkbox-warning checkbox-circle">
              <input id="checkbox11" type="checkbox" disabled="" checked="">
              <label for="checkbox11">
                And this
              </label>
            </div>
          </fieldset>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-12">
      <div class="card">
        <div class="card-header">
          <h6>Radios</h6>
        </div>
        <div class="card-body">
          <fieldset>
            <div class="row">
              <div class="col-sm-6">
                <div class="radio">
                  <input type="radio" name="radio1" id="radio1" value="option1" checked="">
                  <label for="radio1">
                    Small
                  </label>
                </div>
                <div class="radio">
                  <input type="radio" name="radio1" id="radio2" value="option2">
                  <label for="radio2">
                    Big
                  </label>
                </div>
              </div>
              <div class="col-sm-6">
                <div class="radio radio-warning">
                  <input type="radio" name="radio2" id="radio3" value="option1">
                  <label for="radio3">
                    Next
                  </label>
                </div>
                <div class="radio radio-danger">
                  <input type="radio" name="radio2" id="radio4" value="option2" checked="">
                  <label for="radio4">
                    One
                  </label>
                </div>
              </div>
            </div>
            <p>Inline radios</p>
            <div class="radio radio-info form-check form-check-inline">
              <input type="radio" id="inlineRadio1" value="option1" name="radioInline" checked="">
              <label for="inlineRadio1"> Inline One </label>
            </div>
            <div class="radio form-check form-check-inline">
              <input type="radio" id="inlineRadio2" value="option1" name="radioInline">
              <label for="inlineRadio2"> Inline Two </label>
            </div>
          </fieldset>
        </div>
      </div>
    </div>
  </div>
  <div class='row'>
    <div class='col-sm-12'>
      <div class="card card-default" id="input-groups">
        <div class="card-header">
          <h6>Disabled Radios</h6>
        </div>
        <div class="card-body">
          <fieldset>
            <div class="radio form-check form-check-inline">
              <input type="radio" name="radio3" id="radio5" value="option1" disabled="">
              <label for="radio5">
                Next
              </label>
            </div>
            <div class="radio form-check form-check-inline">
              <input type="radio" name="radio3" id="radio6" value="option2" checked="" disabled="">
              <label for="radio6">
                One
              </label>
            </div>
          </fieldset>
        </div>
      </div>
    </div>
  </div>
</div>
